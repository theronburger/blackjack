{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6", "DOM",],
    "checkJs": true,
    "allowJs": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
      "node_modules"
  ],
  "include": [
    "./src/"
  ]
}